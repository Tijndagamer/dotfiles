[cpu_usage]
command=getcpuusage
interval=1

[cpu_temp]
command=getcputemp
interval=1

[mem_usage]
command=getusedmem
interval=1

[disk]
command= getfreespace
interval=30

[networkstatus_wan]
command=getnetworkstatus_wan
interval=15

[networkstatus_lan]
command=getnetworkstatus_lan
interval=15

[battery_level]
command=getbatterystatus
interval=1

[time]
command=date '+%H:%M %d-%m-%Y'
color=#19D1FF
interval=1
